<main id="main" class="main">
  <link
    rel="stylesheet"
    href="https://allyoucan.cloud/cdn/icofont/1.0.1/icofont.css"
    integrity="sha384-jbCTJB16Q17718YM9U22iJkhuGbS0Gd2LjaWb4YJEZToOPmnKDjySVa323U+W7Fv"
    crossorigin="anonymous"
  />

  <div class="container">
    <div class="row">
      <div class="col-md">
        <div class="osahan-account-page-right shadow-sm bg-white p-4 h-100">
          <div class="tab-content" id="myTabContent">
            <div
              class="tab-pane fade active show"
              id="orders"
              role="tabpanel"
              aria-labelledby="orders-tab"
            >
              <h4 class="font-weight-bold mt-0 mb-4">Past Orders</h4>
              {{#each history}}

              <div class="bg-white card mb-4 order-list shadow-sm">
                <div class="gold-members p-4">
                  <a href="#">
                  </a>
                  <div class="media">
                   
                    <div class="media-body">
                      <a href="#">
                        <span class="float-right">{{TIME}}
                          <i
                            class="icofont-check-circled text-success"
                          ></i></span>
                      </a>
                      <h6 class="mb-2">
                        <a href="#"></a>
                        <a href="#" class="text-black">ID: {{IDBILL}}</a>
                      </h6>
                      <p class="text-gray mb-1"><i
                          class="icofont-location-arrow"
                        ></i>
                        STATUS: {{STATUS}}
                      </p>
                      </p>
                      <hr />
                      {{!-- <div class="float-right">
                        <a class="btn btn-sm btn-outline-primary" href="#"><i
                            class="icofont-headphone-alt"
                          ></i>
                          HELP</a>
                        <a class="btn btn-sm btn-primary" href="#"><i
                            class="icofont-refresh"
                          ></i>
                          REORDER</a>
                      </div> --}}
                      <p class="mb-0 text-black text-primary pt-2"><span
                          class="text-black font-weight-bold"
                        > Total Paid:</span>
                        {{PRICE}}
                      </p>
                    </div>
                  </div>

                </div>
              </div>
{{/each}}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</main>